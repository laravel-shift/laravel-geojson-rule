{
    "name": "yuca/laravel-geojson-rule",
    "type": "library",
    "description": "Laravel Validation Rule for GeoJSON.",
    "keywords": [
        "yuca",
        "laravel",
        "geojson"
    ],
    "homepage": "https://github.com/yuca/laravel-geojson-rule",
    "license": "MIT",
    "authors": [
        {
            "name": "Hrvoje Jukic",
            "email": "hrcajuka@gmail.com",
            "homepage": "https://github.com/yuca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "jmikola/geojson": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Yuca\\LaravelGeoJsonRule\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuca\\LaravelGeoJsonRule\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "thanks": {
            "name": "jmikola/geojson",
            "url": "https://github.com/jmikola/geojson"
        }
    },
    "config": {
        "sort-packages": true
    }
}
